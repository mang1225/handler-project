<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geniusmart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".one.HandlerActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".two.HandlerThreadActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".two.HandlerThreadActivity2"
            android:label="@string/app_name" />
        <activity
            android:name=".three.AsyncTaskActivity"
            android:label="@string/title_activity_async_task" />
        <activity
            android:name=".event.ignorant.EventActivity"
            android:label="@string/title_activity_event"></activity>
        <activity
            android:name=".event.interested.EventActivity2"
            android:label="@string/title_activity_event"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".future.FutureActivity"></activity>
    </application>

</manifest>